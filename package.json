{
  "name": "hashland",
  "private": true,
  "description": "",
  "license": "Unlicense",
  "sideEffects": false,
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "run-s build:css build:remix",
    "build:css": "cross-env NODE_ENV=production npm run generate:css -- --no-map",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development wrangler dev ./build/index.js --local",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- -w",
    "generate:css": "postcss ./styles/main.css -o ./app/main.css",
    "deploy": "wrangler publish"
  },
  "dependencies": {
    "@giscus/react": "^2.2.0",
    "@iconify-icon/react": "^1.0.0-beta.3",
    "@iconify-icons/heroicons-outline": "^1.2.4",
    "@iconify-icons/heroicons-solid": "^1.2.5",
    "@remix-run/cloudflare": "^1.7.0",
    "@remix-run/cloudflare-workers": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "feed": "^4.2.2",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "prism-theme-vars": "^0.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.22"
  },
  "engines": {
    "node": ">=16.13"
  }
}